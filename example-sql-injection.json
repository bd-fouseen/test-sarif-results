  "version": "2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "ExampleAnalyzer",
          "informationUri": "https://get.sigma.io",
          "rules": [
            {
              "id": "SQL001",
              "name": "SQL Injection",
              "shortDescription": {
                "text": "Detects potential SQL injection vulnerabilities."
              },
              "properties": {
                "security-severity": "8.5",
                "tags": [
                  "cwe-89"
                ],
                "problem.severity": "error"
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "SQL001",
